{
  "name": "tskeleton",
  "version": "1.0.0",
  "description": "a ts skeleton",
  "main": "dist/index.js",
  "repository": "git@github.com:bnbry/tskeleton.git",
  "author": "Matthew Banbury <mbanbury@gmail.com>",
  "license": "CC0-1.0",
  "private": false,
  "type": "module",
  "scripts": {
    "test": "jest",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "clean": "rm -rf dist build",
    "build": "yarn clean && yarn build:dist",
    "build:dist": "node build.js",
    "build:tsc": "tsc -p tsconfig.json",
    "build:watch": "node build.js --watch=true"
  },
  "devDependencies": {
    "@types/jest": "^28.1.5",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "esbuild": "^0.14.49",
    "eslint": "^8.19.0",
    "jest": "^28.1.3",
    "ts-jest": "^28.0.6",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "esbuild-sass-plugin": "^2.2.6",
    "minimist": "^1.2.6"
  }
}
